{
    "name": "calibrate/caliwatch_lib",
    "description": "A client for the CaliWatch 2.0 API",
    "type": "library",
    "version": "0.7.9",
    "require": {
        "php": "^8.0|^8.1",
        "guzzlehttp/guzzle": "^6.2.1|^7.2",
        "illuminate/contracts": "^7.20|^8.19|^9.0|^10.0",
        "illuminate/support": "^7.20|^8.19|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "calibrate\\caliwatch\\client\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Steven Van den Hout",
            "email": "steven.vandenhout@calibrate.be"
        },
        {
            "name": "Joris Vercammen",
            "email": "joris.vercammen@calibrate.be"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "calibrate\\caliwatch\\client\\Providers\\CaliwatchClientServiceProvider"
            ]
        }
    }
}
